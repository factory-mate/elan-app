{
  "name": "admin",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:staging": "cross-env NODE_ENV=staging vite --mode staging",
    "dev:prod": "cross-env NODE_ENV=production vite --mode production",
    "build": "tsc && vite build",
    "build:staging": "cross-env NODE_ENV=staging vite build --mode staging",
    "build:prod": "cross-env NODE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "type:check": "tsc --pretty --noEmit",
    "cspell:check": "cspell --no-progress --show-suggestions --show-context --cache **",
    "eslint:check": "eslint . --color --cache",
    "eslint:fix": "eslint . --color --cache --fix",
    "prettier:check": "prettier --check --cache --ignore-unknown --ignore-path=../../.prettierignore .",
